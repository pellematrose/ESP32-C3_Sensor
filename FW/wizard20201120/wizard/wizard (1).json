{
  "version": "1.0.2", 
  "api": {
    "reboot": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "mqtt": {"type": "data", "read_level": 0, "write_level": 0, "attributes": {"en": {"type": "bool", "value": true}, "broker": {"type": "string", "value": "yourmqttbroker", "size": 50}, "topic_pressure": {"type": "string", "value": "/pt100/pressure", "size": 30}, "topic_batvol": {"type": "string", "value": "/pt100/batvol", "size": 30}, "topic_temp2": {"type": "string", "value": "/pt100/sensor2", "size": 30}, "topic_temp1": {"type": "string", "value": "/pt100/sensor1", "size": 30}}}, 
    "debug": {
      "type": "data", 
      "read_level": 0, 
      "write_level": 0, 
      "attributes": {
        "boot": {"type": "bool", "value": false}, 
        "ready": {"type": "bool", "value": false}, 
        "charge": {"type": "bool", "value": false}, 
        "op": {"type": "bool", "value": false}, 
        "ldo": {"type": "bool", "value": false}, 
        "led": {"type": "bool", "value": false}
      }
    }, 
    "tempoffset2": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "tempoffset1": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "battery": {
      "type": "data", 
      "read_level": 0, 
      "write_level": 0, 
      "attributes": {
        "voltage": {"type": "int", "value": 42}, 
        "percent": {"type": "int", "value": 55}, 
        "status": {"type": "string", "value": "charging", "size": 10}
      }
    }, 
    "resetcal2": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "resetcal1": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "btnlow2": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "btnhigh2": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "btnhigh": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "btnlow": {
      "type": "action", 
      "read_level": 0, 
      "write_level": 0
    }, 
    "temperature": {
      "type": "data", 
      "read_level": 0, 
      "write_level": 0, 
      "attributes": {
        "press": {"type": "double", "value": 0, "format": "%.1f"}, 
        "temp1": {"type": "double", "value": 0, "format": "%.1f"}, 
        "temp2": {"type": "double", "value": 0, "format": "%.1f"}
      }
    }, 
    "calibration": {
      "type": "data", 
      "read_level": 0, 
      "write_level": 0, 
      "attributes": {
        "tempcal2": {"type": "double", "value": 0, "format": "%.2f"}, 
        "tempcal1": {"type": "double", "value": 0, "format": "%.2f"}, 
        "ench2": {"type": "bool", "value": false}, 
        "ench1": {"type": "bool", "value": false}, 
        "adcl2": {"type": "int", "value": 0}, 
        "adch2": {"type": "int", "value": 0}, 
        "adc2": {"type": "int", "value": 0}, 
        "adcl": {"type": "int", "value": 0}, 
        "adch": {"type": "int", "value": 0}, 
        "adc": {"type": "int", "value": 0}
      }
    }, 
    "wifi": {
      "type": "data", 
      "read_level": 0, 
      "write_level": 0, 
      "attributes": {
        "ssid": {"type": "string", "value": "", "size": 25}, 
        "password": {"type": "string", "value": "", "size": 25}, 
        "save": {"type": "bool", "value": false}
      }
    }, 
    "save_event": {
      "type": "action"
    }, 
    "events": {
      "type": "custom", 
      "value": [
        {
          "id": 0, 
          "timestamp": 12345, 
          "message": "Something failed", 
          "priority": 2, 
          "status": "active", 
          "notes": "some notes"
        },
        {
          "id": 1, 
          "timestamp": 12395, 
          "message": "Something succeeded", 
          "priority": 1, 
          "status": "processed", 
          "notes": ""
        }
      ]
    }
  }, 
  "ui": {
    "production": false, 
    "brand": "Brand Name", 
    "logo": "<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<svg viewBox=\"0 0 600 150\" xmlns=\"http://www.w3.org/2000/svg\">\n  <rect x=\"0\" y=\"0\" width=\"600\" height=\"150\" rx=\"20\" ry=\"20\" style=\"stroke: none; fill: #e1e5e9;\"/>\n  <text style=\"fill: #94A3B8; font-family: Arial, sans-serif; font-size: 92px;dominant-baseline: middle; text-anchor: middle; \" x=\"50%\" y=\"50%\">my logo</text>\n</svg>", 
    "toolbar": {"css": "height: 2rem;\nflex-grow: 1;", "layout": [
        {"classes": "container", "css": "flex-grow: 1;\njustify-content: center;", "layout": [
            {"classes": "container", "css": "align-items: center;", "layout": [
                {"type": "image", "image": "data:image/png;base64,", "css": "height: 2rem;\nalign-items: center;\njustify-content: center;\ndisplay: flex;"}
              ]},
            {"classes": "container", "css": "margin-left: auto; align-items: center", "layout": [
                {"format": "Thermometer"}
              ]}
          ]}
      ]}, 
    "theme": {
    }, 
    "pages": [
      {"title": "Dashboard", "icon": "desktop", "level": 0, "css": "padding: 0.75rem;\ngap: 0.5rem;\nmin-height: 2rem;\ndisplay: flex;\nflex-direction: column;", "layout": [
          {"classes": "container", "css": "gap: 0.5rem;\nflex-wrap: wrap;", "layout": [
              {"classes": "panel", "css": "flex-direction: column;\nflex-basis: 0 0 auto;", "layout": [
                  {"classes": "container", "css": "gap: 0.5rem;", "layout": [
                      {"css": "white-space: nowrap;\ntext-overflow: ellipsis;\nfont-weight: 700;", "format": "Temperature"}
                    ]},
                  {"classes": "container", "css": "justify-content: center;\nalign-items: start;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Temperature 1: ${temperature.temp1}°C"},
                      {"format": "Temperature 2: ${temperature.temp2}°C"},
                      {"format": "Pressure: ${temperature.press}hPa", "css": "height: 1.25rem;"}
                    ]}
                ]}
            ]},
          {"classes": "container", "css": "gap:0.5rem; flex-wrap:wrap;", "layout": [
              {"classes": "panel", "css": "flex-basis: 0 0 auto;", "layout": [
                  {"classes": "container", "css": "gap: 0.5rem;", "layout": [
                      {"css": "white-space: nowrap;\ntext-overflow: ellipsis;\nfont-weight: 700;", "format": "Battery Status"}
                    ]},
                  {"classes": "container", "css": "flex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"type": "progress", "ref": "battery.percent"},
                      {"format": "Battery voltage ${battery.voltage}mV"},
                      {"format": "Staus: ${battery.status}"}
                    ]}
                ]}
            ]}
        ]},
      {"title": "Login", "icon": "feed", "css": "padding: 0.75rem;\ngap: 0.5rem;\nmin-height: 2rem;\ndisplay: flex;\nflex-direction: column;", "layout": [
          {"classes": "container", "css": "gap: 0.5rem;\nflex-wrap: wrap;", "layout": [
              {"classes": "panel", "css": "flex-basis: 0 0 auto;", "layout": [
                  {"classes": "container", "css": "gap: 0.5rem;", "layout": [
                      {"css": "white-space: nowrap;\ntext-overflow: ellipsis;\nfont-weight: 700;", "format": "Connect to Wifi"}
                    ]},
                  {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                      {"format": "SSID"},
                      {"type": "input", "ref": "wifi.ssid", "css": "flex: 0 0 auto;\nwidth: 15rem;", "input": "text"}
                    ]},
                  {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                      {"format": "Password"},
                      {"type": "input", "ref": "wifi.password", "css": "flex: 0 0 auto;\nwidth: 15rem;", "input": "text"}
                    ]},
                  {"css": "justify-content: space-between;\nalign-items: center;\nmargin-top: 0.25rem;\ndisplay: flex;\ngap: 1rem;", "layout": [
                      {"format": "Max 25 characters!"},
                      {"type": "savebutton", "ref": "wifi", "title": "save", "icon": "save", "css": "flex: 0 0 auto;\nwidth: 10rem;"}
                    ]},
                  {"classes": "container", "css": "justify-content: end;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"type": "action", "title": "Connect to Wifi", "icon": "refresh", "ref": "reboot", "css": "flex: 0 0 auto;\nwidth: 10rem;\nbackground: #38842e;"}
                    ]}
                ]}
            ]}
        ]},
      {"title": "MQTT", "icon": "mail", "css": "align-items: start;\npadding: 0.75rem;\ngap: 0.5rem;\nmin-height: 2rem;\ndisplay: flex;\nflex-direction: column;", "layout": [
          {"classes": "panel", "css": "flex-basis: 0 0 auto;", "layout": [
              {"classes": "container", "css": "justify-content: space-between;\ngap: 0.5rem;", "layout": [
                  {"css": "white-space: nowrap;\ntext-overflow: ellipsis;\nfont-weight: 700;", "format": "MQTT Config"},
                  {"type": "toggle", "ref": "mqtt.en", "autosave": true}
                ]},
              {"css": "justify-content: space-between;\nalign-items: center;\ndisplay: flex;\ngap: 1rem;", "layout": [
                  {"format": "Broker address"},
                  {"type": "input", "ref": "mqtt.broker", "css": "flex: 0 0 auto;\nwidth: 8rem;", "disabled": "${mqtt.en == false}"}
                ]},
              {"css": "justify-content: space-between;\nalign-items: center;\ndisplay: flex;\ngap: 1rem;", "layout": [
                  {"format": "Topic Temp1"},
                  {"type": "input", "ref": "mqtt.topic_temp1", "css": "flex: 0 0 auto;\nwidth: 8rem;", "disabled": "${mqtt.en == false}"}
                ]},
              {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                  {"format": "Topic Temp2"},
                  {"type": "input", "ref": "mqtt.topic_temp2", "css": "flex: 0 0 auto;\nwidth: 8rem;", "disabled": "${mqtt.en == false}"}
                ]},
              {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                  {"format": "Topic Battery Voltage"},
                  {"type": "input", "ref": "mqtt.topic_batvol", "css": "flex: 0 0 auto;\nwidth: 8rem;", "disabled": "${mqtt.en == false}"}
                ]},
              {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                  {"format": "Topic Air pressure"},
                  {"type": "input", "ref": "mqtt.topic_pressure", "css": "flex: 0 0 auto;\nwidth: 8rem;", "disabled": "${mqtt.en == false}"}
                ]},
              {"css": "margin-top: 0.25rem; justify-content:end;display:flex; align-items:center; gap: 1rem;", "layout": [
                  {"type": "savebutton", "ref": "mqtt", "title": "save", "icon": "save", "disabled": "${mqtt.en == false}"}
                ]},
              {"format": "Max. 30 characters", "css": "color: #f50000;"}
            ]}
        ]},
      {"title": "Calibration", "icon": "activity", "css": "padding: 0.75rem;\ngap: 0.5rem;\nmin-height: 2rem;\ndisplay: flex;\nflex-direction: column;", "layout": [
          {"classes": "container", "css": "gap: 0.5rem;\nflex-wrap: wrap;", "layout": [
              {"classes": "panel", "css": "flex-basis: 0 0 auto;", "layout": [
                  {"classes": "container", "css": "justify-content: space-between;\ngap: 0.5rem;", "layout": [
                      {"css": "white-space: nowrap;\ntext-overflow: ellipsis;\nfont-weight: 700;", "format": "Calibration Channel 1"},
                      {"type": "toggle", "ref": "calibration.ench1", "autosave": true}
                    ]},
                  {"classes": "container", "css": "gap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Voltage:"},
                      {"format": "${calibration.adc}mV"}
                    ]},
                  {"classes": "container", "css": "padding: 1rem 0.5rem;\nborder-radius: .5rem;\nborder: 1px solid #2563eb;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Set R to 95 Ohms and click save"},
                      {"type": "action", "title": "Save ADC Value", "icon": "arrow-bottom", "ref": "btnlow", "params": "", "disabled": "${calibration.ench1 == false}"},
                      {"format": "Stored value: ${calibration.adcl}mV"}
                    ]},
                  {"classes": "container", "css": "padding: 1rem 0.5rem;\nborder-radius: .5rem;\nborder: 1px solid #2563eb;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Set R to 115 Ohms and click save"},
                      {"type": "action", "title": "Save ADC Value", "icon": "arrow-bottom", "ref": "btnhigh", "disabled": "${calibration.ench1 == false}"},
                      {"format": "Stored value: ${calibration.adch}mV"}
                    ]},
                  {"classes": "container", "css": "padding: 1rem 0.5rem;\nborder-radius: .5rem;\nborder: 1px solid #2563eb;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Set Temperature to 0°C and click save"},
                      {"type": "action", "title": "Save Temperature", "icon": "arrow-bottom", "ref": "tempoffset1", "disabled": "${calibration.ench1 == false}"},
                      {"format": "Stored value: ${calibration.tempcal1}°C"}
                    ]},
                  {"type": "action", "title": "Reset calibration", "icon": "refresh", "ref": "resetcal1", "css": "color: #e3e3e3;\nbackground: #e12323;"}
                ]}
            ]},
          {"classes": "container", "css": "gap: 0.5rem;\nflex-wrap: wrap;", "layout": [
              {"classes": "panel", "css": "flex-basis: 0 0 auto;", "layout": [
                  {"classes": "container", "css": "justify-content: space-between;\ngap: 0.5rem;", "layout": [
                      {"css": "white-space: nowrap;\ntext-overflow: ellipsis;\nfont-weight: 700;", "format": "Calibration Channel 2"},
                      {"type": "toggle", "ref": "calibration.ench2", "autosave": true}
                    ]},
                  {"classes": "container", "css": "gap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Voltage:"},
                      {"format": "${calibration.adc2}mV"}
                    ]},
                  {"classes": "container", "css": "padding: 1rem 0.5rem;\nborder-radius: .5rem;\nborder: 1px solid #2563eb;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Set R to 95 Ohms and click save"},
                      {"type": "action", "title": "Save ADC Value", "icon": "arrow-bottom", "ref": "btnlow2", "params": "", "disabled": "${calibration.ench2 == false}"},
                      {"format": "Stored value: ${calibration.adcl2}mV"}
                    ]},
                  {"classes": "container", "css": "padding: 1rem 0.5rem;\nborder-radius: .5rem;\nborder: 1px solid #2563eb;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Set R to 115 Ohms and click save"},
                      {"type": "action", "title": "Save ADC Value", "icon": "arrow-bottom", "ref": "btnhigh2", "disabled": "${calibration.ench2 == false}"},
                      {"format": "Stored value: ${calibration.adch2}mV"}
                    ]},
                  {"classes": "container", "css": "padding: 1rem 0.5rem;\nborder-radius: .5rem;\nborder: 1px solid #2563eb;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Set Temperature to 0°C and click save"},
                      {"type": "action", "title": "Save Temperature", "icon": "arrow-bottom", "ref": "tempoffset2", "disabled": "${calibration.ench2 == false}"},
                      {"format": "Stored value: ${calibration.tempcal2}°C"}
                    ]},
                  {"type": "action", "title": "Reset calibration", "icon": "refresh", "ref": "resetcal2", "css": "color: #e3e3e3;\nbackground: #e12323;"}
                ]}
            ]}
        ]},
      {"title": "Debug", "icon": "work", "css": "padding: 0.75rem;\ngap: 0.5rem;\nmin-height: 2rem;\ndisplay: flex;\nflex-direction: column;\nflex-grow: 1;", "layout": [
          {"classes": "container", "css": "gap: 0.5rem;\nflex-wrap: wrap;", "layout": [
              {"classes": "panel", "css": "flex-basis: 0 0 auto;", "layout": [
                  {"classes": "container", "css": "gap: 0.5rem;", "layout": [
                      {"css": "white-space: nowrap;\ntext-overflow: ellipsis;\nfont-weight: 700;", "format": "Debug"}
                    ]},
                  {"classes": "container", "css": "padding-left: 0.5rem;\npadding-bottom: 0.5rem;\npadding-right: 0.5rem;\nborder-radius: 0.5rem;\nborder: 1px solid #2563eb;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Output Pins"},
                      {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                          {"format": "OP Pin 6"},
                          {"type": "toggle", "ref": "debug.op", "autosave": true}
                        ]},
                      {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                          {"format": "LDO Pin 7"},
                          {"type": "toggle", "ref": "debug.ldo", "autosave": true}
                        ]},
                      {"css": "display: flex;\nalign-items: center;\njustify-content: space-between;\ngap: 1rem;", "layout": [
                          {"format": "LED Pin 8"},
                          {"type": "toggle", "ref": "debug.led", "autosave": true}
                        ]}
                    ]},
                  {"classes": "container", "css": "padding-left: 0.5rem;\npadding-bottom: 0.5rem;\npadding-right: 0.5rem;\nborder: 1px solid #2563eb;\nborder-radius: 0.5rem;\nflex-direction: column;\ngap: 0.5rem;\nflex-wrap: wrap;", "layout": [
                      {"format": "Input Pins"},
                      {"format": "Ready Pin 0: ${debug.ready}"},
                      {"format": "Boot Button 9: ${debug.boot}"},
                      {"format": "Charge Pin 10: ${debug.charge}"}
                    ]}
                ]}
            ]}
        ]}
    ]
  }, 
  "http": {"http": true, "https": false, "ui": true, "login": false, "ca": ""}, 
  "mqtt": {"enable": false, "url": "mqtt://broker.hivemq.com:1883", "ca": "", "rx": "{device_id}/rx", "tx": "{device_id}/tx"}, 
  "dns": {"type": "default", "url": "udp://8.8.8.8:53", "captive": false}, 
  "sntp": {"enable": false, "type": 0, "url": "udp://time.google.com:123", "interval": 3600}, 
  "modbus": {"enable": false, "port": 502}, 
  "build": {"mode": "existing", "board": "unix", "ide": "GCC+make", "rtos": "baremetal"}, 
  "websocket": {"enable": false}
